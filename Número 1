programa
 {
	
	funcao inicio()
 {

inteiro A, B, C, soma

// Solicitando dados

escreva("Digite o valor de A: ")
leia(A)
escreva("Digite o valor de B: ")
leia(B)
escreva("Digite o valor de C: ")
leia(C)

// Calculando os dados

soma = (A + B)

// Função se senao
se(soma < C) {
    escreva("A + B é menor que C")
}
senao {
    escreva("A + B é maior que C")
}

	}
}
